    "intl",
    "i18n",
    "internationalization",
    "locale",
    "localization",
    "globalization",
    "react-intl",
    "format",
    "formatting",
    "translate",
    "translation"
  ],
  "author": "Long Ho <holevietlong@gmail.com>",
  "license": "MIT",
  "homepage": "https://formatjs.io",
  "bugs": {
    "url": "https://github.com/formatjs/formatjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:formatjs/formatjs.git"
  },
  "main": "index.js",
